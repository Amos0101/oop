class Animal:
    def move(self):
        print("The animal moves in some way.")

class Bird(Animal):
    def move(self):
        print("Flying in the sky")

class Fish(Animal):
    def move(self):
        print("Swimming in the water")

class Dog(Animal):
    def move(self):
        print("Running on the ground ")


animals = [Bird(), Fish(), Dog()]

for animal in animals:
    animal.move()
